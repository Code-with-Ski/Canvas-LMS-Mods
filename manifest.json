{
  "name": "Canvas LMS Mods (Basic)",
  "description": "This provides modifications to the Canvas LMS to improve the user experience visually and functionally.",
  "version": "3.0.0",
  "version_name": "3.0.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "host_permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "global/global.js",
        "global/global-nav.js",
        "utils/common/ski-canvas-lms-api-calls.js",
        "utils/common/ski-monitor-changes.js"
      ],
      "css": [
        "global/global.css",
        "utils/ski-ui-elements/ski-ui-styles.css"
      ]
    },
    {
      "matches": [
        "https://*.test.instructure.com/*"
      ],
      "js": [
        "global/global-test.js"
      ],
      "css": ["global/global-test.css"]
    },
    {
      "matches": [
        "https://*.beta.instructure.com/*"
      ],
      "js": [
        "global/global-beta.js"
      ],
      "css": ["global/global-beta.css"]
    },
    {
      "matches": [
        "https://*/"
      ],
      "js": [
        "canvas-mods/dashboard-home/feature-all-courses-link/all-courses-link.js",
        "canvas-mods/dashboard-home/feature-grades-on-course-cards/grades-on-course-cards.js"
      ]
    },
    {
      "matches": [
        "https://*/profile"
      ],
      "js": ["account/profile.js"]
    },
    {
      "matches": [
        "https://*/courses"
      ],
      "js": ["courses/all-courses.js"],
      "css": ["courses/all-courses.css"]
    },
    {
      "matches": [
        "https://*/courses/*"
      ],
      "js": ["course/course-global.js"],
      "css": ["course/course-global.css"]
    },
    {
      "matches": [
        "https://*/courses/*/modules"
      ],
      "js": ["course/modules.js"]
    },
    {
      "matches": [
        "https://*/courses/*/assignments/*"
      ],
      "js": ["canvas-mods/courses/assignments/feature-export-grades/assignment-export-grades.js"]
    },
    {
      "matches": [
        "https://*/courses/*/statistics"
      ],
      "js": [
        "utils/ski-ui-elements/ski-table-elements/ski-table-config.js",
        "utils/ski-ui-elements/ski-table-elements/ski-table-heading-config.js",
        "utils/ski-ui-elements/ski-table-elements/ski-table-data-config.js",
        "utils/ski-ui-elements/ski-table-elements/ski-table.js",
        "utils/ski-ui-elements/ski-report/ski-report.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-announcements.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-assignments.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-discussion-replies.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-discussions.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-modules-progress.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-pages.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-quizzes.js",
        "utils/ski-ui-elements/ski-report/ski-report-course/ski-report-course-submissions.js",
        "canvas-mods/courses/statistics/feature-course-reports/course-reports.js"
      ],
      "css": [
        "utils/ski-ui-elements/ski-table-elements/ski-table-elements.css"
      ]
    },
    {
      "matches": [
        "https://*/courses/*/groups*"
      ],
      "js": ["course/groups.js"]
    },
    {
      "matches": [
        "https://*/courses/*/users"
      ],
      "js": ["course/people.js"],
      "css": ["course/people.css"]
    },
    {
      "matches": [
        "https://*/courses/*/users/*/usage"
      ],
      "js": ["course/user-access-report.js"]
    },
    {
      "matches": [
        "https://*/courses/*/gradebook/speed_grader*"
      ],
      "css": ["course/speedgrader.css"],
      "js": ["course/speedgrader.js"]
    },
    {
      "matches": [
        "https://*/users/*"
      ],
      "js": ["admin/user.js"]
    },
    {
      "matches": [
        "https://*/users/*/grades"
      ],
      "js": ["admin/user-grades.js"]
    },
    {
      "matches": [
        "https://*/accounts/*/users/*"
      ],
      "js": ["admin/user.js"]
    },
    {
      "matches": [
        "https://*/accounts/*/avatars*"
      ],
      "js": ["admin/user-profile-picture-review.js"]
    },
    {
      "matches": [
        "https://*/accounts/*"
      ],
      "js": ["admin/course-search.js"],
      "css": ["admin/course-search.css"]
    },
    {
      "matches": [
        "https://*/accounts/*/terms"
      ],
      "js": ["admin/terms.js"]
    },
    {
      "matches": [
        "https://*/accounts/*/rubrics"
      ],
      "js": ["admin/rubrics.js"]
    },
    {
      "matches": [
        "https://*/accounts/*/question_banks"
      ],
      "js": ["admin/question-banks.js"]
    },
    {
      "matches": [
        "https://*/accounts/*/sub_accounts"
      ],
      "js": ["admin/sub-accounts.js"]
    },
    {
      "matches": [
        "https://*/accounts/*/sis_import"
      ],
      "js": ["admin/sis-import.js"],
      "css": ["admin/sis-import.css"]
    },
    {
      "matches": [
        "https://*/accounts/*/rubrics*",
        "https://*/courses/*/rubrics*"
      ],
      "js": [
        "canvas-mods/shared-locations/rubrics/feature-drag-and-drop-rubric-rows/drag-and-drop-rubric-rows.js",
        "canvas-mods/shared-locations/rubrics/feature-import-rubric/import-rubric.js"
      ]
    }
  ],
  "icons": {
    "16": "/images/extension_logo16.png",
    "32": "/images/extension_logo32.png",
    "48": "/images/extension_logo48.png",
    "128": "/images/extension_logo128.png"
  },
  "options_page": "options.html"
}
